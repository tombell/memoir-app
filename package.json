{
  "name": "memoir-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006 --no-open",
    "lint": "eslint src",
    "prepare": "husky install"
  },
  "dependencies": {
    "@preact/signals": "^1.1.1",
    "@storybook/preact": "^6.5.12",
    "preact": "^10.11.0",
    "preact-router": "^4.1.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.3.0",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/builder-vite": "^0.2.4",
    "@types/node": "^18.8.3",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "rustywind": "^0.15.1",
    "storybook-addon-mock": "^3.1.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vite-tsconfig-paths": "^3.5.1"
  },
  "lint-staged": {
    "src/**/*.css": [
      "rustywind --custom-regex '@apply ([_a-zA\\.-Z0-9\\s\\-:\\[\\]]+);' --write",
      "prettier --write"
    ],
    "src/**/*.{ts,tsx,js,jsx}": [
      "rustywind --write",
      "prettier --write"
    ]
  }
}
